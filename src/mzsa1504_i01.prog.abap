MODULE user_command_0100 INPUT.
  CASE sy-ucomm.
    WHEN 'EXIT'.
      LEAVE PROGRAM.
      WHEN 'CANC'.
        SET SCREEN 0.
        LEAVE PROGRAM.
        WHEN 'BACK'.
          LEAVE TO SCREEN 0.
  ENDCASE.
ENDMODULE.
